  function maybe_check_vote_990317(element, value) {
    reset_labels();
    if(value.length > 1 && value.length < 30 && validate_user_vote_990317()) {
      jQuery('#spinner_vote_990317').show();
      jQuery.get('/home/check_field_availability', { model : 'user', field : 'login', value : value} , function(data) {
        jQuery('#spinner_vote_990317').hide();
