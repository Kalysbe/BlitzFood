  function validate_user_vote_990317() {
    regexp = /^[A-Za-z0-9]{1}[A-Za-z0-9-]*[A-Za-z0-9]{1}$/i;
    value  = $('user_to_create_login_vote_990317').value
    if (!lenght_ok(value)) {return true;}
    else {
      if (value.match(/^\d+$/)) {
        jQuery('#more_error_vote_990317').text('должен содержать хотя бы одну букву ').css("color","red").show();
        return false;
      }
      if (!regexp.test(value)) {
        jQuery('#more_error_vote_990317').text('должен состоять из букв A-Z, a-z, цифр 0-9 (Внимание: пробелы не допускаются!). Не может начинаться или заканчиваться символом "-", и не должен состоять только из цифр.').css("color","red").show();
        return false;
      }
      else {
        return true;
      }

    }
  }
