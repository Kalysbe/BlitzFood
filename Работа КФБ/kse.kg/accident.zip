<script type="text/javascript">
  function maybe_check_comment(element, value) {
    reset_labels();
    if(value.length > 1 && value.length < 30 && validate_user_comment()) {
      jQuery('#spinner_comment').show();
      jQuery.get('/home/check_field_availability', { model : 'user', field : 'login', value : value} , function(data) {
        jQuery('#spinner_comment').hide();
        if(data == 'true') {
          jQuery('#ok_label_comment').show();
        } else if (data == 'false'){
          jQuery('#not_ok_label_comment').show();
        } else if (data == 'error') { }
      });
      return true;
    }
    else if (value.length <= 1) {
      jQuery('#too_short_label_comment').show();
      return false;
    }
    else if (value.length > 30){
      jQuery('#too_long_label_comment').show();
      return false;
    }
  }
  function lenght_ok(value) { if (value.length > 1 && value.length < 30) return true;}
  function reset_labels() { jQuery('.observable_field span:gt(0)').hide(); jQuery('#more_error_comment').hide();}
  function validate_user_comment() {
    regexp = /^[A-Za-z0-9]{1}[A-Za-z0-9-]*[A-Za-z0-9]{1}$/i;
    value  = $('user_to_create_login_comment').value
    if (!lenght_ok(value)) {return true;}
    else {
      if (value.match(/^\d+$/)) {
        jQuery('#more_error_comment').text('должен содержать хотя бы одну букву ').css("color","red").show();
        return false;
      }
      if (!regexp.test(value)) {
        jQuery('#more_error_comment').text('должен состоять из букв A-Z, a-z, цифр 0-9 (Внимание: пробелы не допускаются!). Не может начинаться или заканчиваться символом "-", и не должен состоять только из цифр.').css("color","red").show();
        return false;
      }
      else {
        return true;
      }

    }
  }
</script>

