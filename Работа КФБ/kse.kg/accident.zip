<script type="text/javascript">
  
  function checkSignupCheckBoxes_fb_comment() {
    if ($('tos_fb_comment').checked == false)
    {
      alert ('Чтобы зарегистрироваться, Вы должны ознакомиться и согласиться с Пользовательским Соглашением');
