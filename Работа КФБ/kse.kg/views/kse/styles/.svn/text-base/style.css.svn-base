.sf-menu, .sf-menu * {
	margin:			0;
	padding:		0;
	list-style:		none;
}
.sf-menu {
	line-height:	1.0;
}
.sf-menu ul {
	position:		absolute;
	top:			-999em;
	width:			230px; /* left offset of submenus need to match (see below) */
}
.sf-menu ul li {
	width:			100%;
}
.sf-menu li:hover {
	visibility:		inherit; /* fixes IE7 'sticky bug' */
}
.sf-menu li {
	float:			left;
	position:		relative;
}
.sf-menu a {
	display:		block;
	position:		relative;
}
.sf-menu li:hover ul,
.sf-menu li.sfHover ul {
	left:			0;
	top:			29px; /* match top ul list item height */
	z-index:		99;
}
ul.sf-menu li:hover li ul,
ul.sf-menu li.sfHover li ul {
	top:			-999em;
}
ul.sf-menu li li:hover ul,
ul.sf-menu li li.sfHover ul {
	left:			230px; /* match ul width */
	top:			0;
}
ul.sf-menu li li:hover li ul,
ul.sf-menu li li.sfHover li ul {
	top:			-999em;
}
ul.sf-menu li li li:hover ul,
ul.sf-menu li li li.sfHover ul {
	left:			230px; /* match ul width */
	top:			0;
}

div.sf-menu {
	width:922px;
	margin:0 auto;
	background:url('/views/kse/images/topmenu_bg.jpg') repeat-x left top;
}


ul.sf-menu li {
        /*height:40px;*/
	text-align:center;
        background-color:#FFFFFF;
        /*border-left: 1px solid #83A1B9;
        border-right: 1px solid #83A1B9;
        border-: 1px solid white;*/
        
        padding: 0px;
        margin: 0px;
        
        border-bottom: 1px dashed #83A1B9;
}

ul.sf-menu ul {
    border: 1px solid #83A1B9;
}

ul.sf-menu>li {
    background:url('/views/kse/images/topmenu_bg.jpg') repeat-x left top;
    border: none;
    padding: 0px;
    margin: 0px;
    height: 29px;
    text-transform:uppercase;
}

.sf-menu .top_element {
    text-transform:uppercase;
    color: #FFFFFF;
}

ul.sf-menu li a {
	width:230px;
	text-decoration:none;
	
        color: #000000;
