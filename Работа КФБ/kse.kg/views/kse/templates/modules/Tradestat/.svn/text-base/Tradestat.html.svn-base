<h2>{{ lang['Last Trade Results'] }}</h2>
<br /><br />
<div id="tradestat_last" class="tradestat_table">
    <h3>{{ lang['Trades for'] }} {{ Date }}</h3>
    <table class="class1" width="100%">
        <tr>
            <th width="*"></th>
            <th width="23%">{{ lang['{MlnSom}'] }}</th>
            <th width="23%">%</th>
            <th width="5%"></th>
        </tr>
        <tr>
            <td>Объем торгов</td>
            <td class="center">{{ Last['TotalVolume'] }}</td>
            <td class="center">{{ Last['TotalVolumePercent'] }}</td>
            <td class="center">{{ Last['TotalVolumeImage'] }}</td>
        </tr>
        <tr>
            <td>Первичный</td>
            <td class="center">{{ Last['Primary'] }}</td>
            <td class="center">{{ Last['PrimaryPercent'] }}</td>
            <td class="center">{{ Last['PrimaryImage'] }}</td>
        </tr>
        <tr>
            <td>Вторичный</td>
            <td class="center">{{ Last['Secondary'] }}</td>
            <td class="center">{{ Last['SecondaryPercent'] }}</td>
            <td class="center">{{ Last['SecondaryImage'] }}</td>
        </tr>
        <tr>
            <td>Листинг</td>
            <td class="center">{{ Last['Listing'] }}</td>
            <td class="center">{{ Last['ListingPercent'] }}</td>
            <td class="center">{{ Last['ListingImage'] }}</td>
        </tr>
        <tr>
            <td>Нелистинг</td>
            <td class="center">{{ Last['NonListing'] }}</td>
            <td class="center">{{ Last['NonListingPercent'] }}</td>
            <td class="center">{{ Last['NonListingImage'] }}</td>
        </tr>
    </table>
</div>

