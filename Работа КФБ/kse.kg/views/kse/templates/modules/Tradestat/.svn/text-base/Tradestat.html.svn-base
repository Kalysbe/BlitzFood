            <td class="center">{{ Last['PrimaryImage'] }}</td>
        </tr>
        <tr>
            <td>Вторичный</td>
            <td class="center">{{ Week['Secondary'] }}</td>
            <td class="center">{{ Week['SecondaryPercent'] }}</td>
            <td class="center">{{ Last['SecondaryImage'] }}</td>
