{% if ListingError %}

<div class="error ui-corner-all">
    {{ ListingError }}
</div>

{% else %}

<table class="class1" width="45%" style="float: left;">
    <tr>
        <th>Общая информация</th>
        <th>&nbsp;</th>
    </tr>
    <tr>
        <td>Наименование компании</td>
        <td>{{ CompanyInfo['CompanyName'] }}</td>
    </tr>
    <tr>
        <td>Ценная бумага</td>
        <td>{{ CompanyInfo['Security'] }}</td>
    </tr>
    <tr>
        <td>Торговые символы</td>
        <td>{{ CompanyInfo['Symbols'] }}</td>
    </tr>
    <tr>
        <td>Отрасль</td>
        <td>{{ CompanyInfo['Sphere'] }}</td>
    </tr>
    <tr>
        <td>Вид деятельности</td>
        <td>{{ CompanyInfo['Activity'] }}</td>
    </tr>
    <tr>
        <td>Дата прохождения листинга </td>
        <td>{{ CompanyInfo['ListingDate'] }}</td>
    </tr>
    <tr>
        <td>ФИО руководителя</td>
        <td>{{ CompanyInfo['Owner'] }}</td>
    </tr>
    <tr>
        <td>Должность руководителя </td>
        <td>{{ CompanyInfo['OwnerPosition'] }}</td>
    </tr>
    <tr>
        <td>Адрес</td>
        <td>{{ CompanyInfo['Address'] }}</td>
    </tr>
    <tr>
        <td>Телефон / Факс </td>
        <td>{{ CompanyInfo['Phone'] }}</td>
    </tr>
    <tr>
        <td>Аудитор </td>
        <td>{{ CompanyInfo['Auditor'] }}</td>
    </tr>
    <tr>
        <td>Регистратор </td>
        <td>{{ CompanyInfo['Registrar'] }}</td>
    </tr>
    <tr>
        <td>Маркет-мейкер </td>
        <td>{{ CompanyInfo['MarketMaker'] }}</td>
    </tr>
</table>

{% if Logged %}
    {% include 'modules/Listing/UploadNewReport.html' %}
{% endif %}

<script type="text/javascript">

    $(function() {
        $("#hidden_balance").dialog({
            autoOpen: false,
            height: 500,
            width: 500,
            modal: true,
            buttons: {           
                "Закрыть": function() {  
                    $(this).dialog('close');
                }
            },
            close: function() {
                allFields.val('').removeClass('ui-state-error');
            }
        });
        $("#hidden_prospect").dialog({
            autoOpen: false,
            height: 500,
            width: 500,
            modal: true,
