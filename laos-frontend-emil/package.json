    "dev": "npm run serve",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/store/i18n/messages/locales/*.json\" --output \"./src/i18n.report.json\"",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run dev",
